## Customization
NEXT_PUBLIC_LOGO_PATH='/logos/logo.svg'
NEXT_PUBLIC_NAME=CloudNet

## Self-hosted domain name
NEXT_PUBLIC_DOMAIN=http://localhost:3000

## Cloudnet server address
NEXT_PUBLIC_CLOUDNET_ADDRESS=
## Hide the Cloudnet server address from input fields
NEXT_PUBLIC_CLOUDNET_ADDRESS_HIDDEN=false

## WebSocket configuration for Docker environments
## Set to true if the CloudNet server is only accessible from within Docker network
NEXT_PUBLIC_WEBSOCKET_USE_PROXY=false
## Alternative WebSocket address for Docker environments (if different from CLOUDNET_ADDRESS)
NEXT_PUBLIC_WEBSOCKET_ADDRESS=

# Use Sentry
SENTRY_URL=
SENTRY_ORG=
SENTRY_PROJECT=
SENTRY_DSN=
SENTRY_AUTH_TOKEN=
SENTRY_SPOTLIGHT=false
